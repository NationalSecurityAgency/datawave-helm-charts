{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "additionalProperties": false,
    "properties": {
        "mysql": {
            "properties": {
                "configMap": {
                    "properties": {
                        "init": {
                            "items": {
                                "properties": {
                                    "fileContents": {
                                        "type": "string"
                                    },
                                    "fileName": {
                                        "type": "string"
                                    },
                                    "name": {
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            },
                            "type": "array"
                        },
                        "name": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "user": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "mysqlDeployment": {
            "properties": {
                "image": {
                    "properties": {
                        "name": {
                            "type": "string"
                        },
                        "pullPolicy": {
                            "type": "string"
                        },
                        "tag": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "livenessProbe": {
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "failureThreshold": {
                            "type": "integer"
                        },
                        "initialDelaySeconds": {
                            "type": "integer"
                        },
                        "periodSeconds": {
                            "type": "integer"
                        },
                        "successThreshold": {
                            "type": "integer"
                        },
                        "timeoutSeconds": {
                            "type": "integer"
                        }
                    },
                    "type": "object"
                },
                "meta": {
                    "properties": {
                        "name": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "readinessProbe": {
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "failureThreshold": {
                            "type": "integer"
                        },
                        "initialDelaySeconds": {
                            "type": "integer"
                        },
                        "periodSeconds": {
                            "type": "integer"
                        },
                        "successThreshold": {
                            "type": "integer"
                        },
                        "timeoutSeconds": {
                            "type": "integer"
                        }
                    },
                    "type": "object"
                },
                "replication": {
                    "properties": {
                        "maxSurge": {
                            "type": "integer"
                        },
                        "maxUnavailable": {
                            "type": "integer"
                        },
                        "replicaCount": {
                            "type": "integer"
                        },
                        "revisionHistoryLimit": {
                            "type": "integer"
                        }
                    },
                    "type": "object"
                },
                "restartPolicy": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "mysqlService": {
            "properties": {
                "meta": {
                    "properties": {
                        "name": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "ports": {
                    "properties": {
                        "mysql": {
                            "type": "integer"
                        }
                    },
                    "type": "object"
                }
            },
            "type": "object"
        }
    },
    "type": "object"
}
